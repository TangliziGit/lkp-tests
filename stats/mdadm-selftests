#!/usr/bin/awk -f

BEGIN {
	FS = "\\.\\.\\. "
}

$0 ~ FS && $2 == "" {
	printf("%s.fail: 1\n", substr($1, 39))
}

$2 ~ /FAILED/ {
	printf("%s.fail: 1\n", substr($1, 39))
}

$2 ~ /succeeded/ {
	printf("%s.pass: 1\n", substr($1, 39))
}

$2 ~ /ignored_by_lkp/ {
       printf("%s.ignored_by_lkp: 1\n", $1)
}

